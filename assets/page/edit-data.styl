body, html
  margin 0
  padding 0
  background #f9f9f9

#edit-wrapper
  margin 0 1em
  padding-bottom 2em

#edit-wrapper h2
  font-size 16px
  font-weight normal
  padding 1em 0 1em 1em
  border-bottom solid 1px #ccc

#edit-wrapper .excel-wrapper
  min-height 200px
  margin 1em auto
  text-align left

#edit-wrapper .excel-wrapper .fields span
  width 200px